<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mario.raizin">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginorSignUpActivity" />
        <activity
            android:name=".userInfo"
            android:parentActivityName=".LoginorSignUpActivity" />
        <activity android:name=".SkinTypeDeterminationActivity" />
        <activity android:name=".HomeFeed" />
        <activity android:name=".GeneralInformationActivity" />
        <activity android:name=".TimeOutsideActivity" />
        <activity android:name=".SkinTypeDeterminationActivityContinuation" />
        <activity android:name=".LoginVerification"></activity>
    </application>

</manifest>